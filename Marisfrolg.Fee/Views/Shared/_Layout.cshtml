        /** 数字金额大写转换(可以处理整数,小数,负数) */
        var num2rmb = function (n) {
            var fraction = ['角', '分'];
            var digit = [
                '零', '壹', '贰', '叁', '肆',
                '伍', '陆', '柒', '捌', '玖'
            ];
            var unit = [
                ['元', '万', '亿'],
                ['', '拾', '佰', '仟']
            ];
            var head = n < 0 ? '欠' : '';
            n = Math.abs(n);
            var s = '';
            for (var i = 0; i < fraction.length; i++) {
                s += (digit[Math.floor(n * 10 * Math.pow(10, i)) % 10] + fraction[i]).replace(/零./, '');
            }
            s = s || '整';
            n = Math.floor(n);
            for (var i = 0; i < unit[0].length && n > 0; i++) {
                var p = '';
                for (var j = 0; j < unit[1].length && n > 0; j++) {
                    p = digit[n % 10] + unit[1][j] + p;
                    n = Math.floor(n / 10);
                }
                s = p.replace(/(零.)*零$/, '').replace(/^$/, '零') + unit[0][i] + s;
            }
            return head + s.replace(/(零.)*零元/, '元')
                .replace(/(零.)+/g, '零')
                .replace(/^整$/, '零元整');
        };

        /** 根据货币简写得到货币图标 */
        function Currency2Icon(Currency) {
            switch (Currency) {
                case "CNY":
                    return "CNY ";
                case "EUR":
                    return "EUR ";
                case "GBP":
                    return "GBP ";
                case "HKD":
                    return "HKD ";
                case "JPY":
                    return "JPY ";
                case "KRW":
                    return "KRW ";
                case "MOP":
                    return "MOP ";
                case "SGD":
                    return "SGD ";
                case "TWD":
                    return "TWD ";
                case "USD":
                    return "USD ";
                case "NZD":
                    return "NZD ";
                default:
            }
        }

        function toDecimal2(x) {
            var f = parseFloat(x);
            if (isNaN(f)) {
                return false;
            }
            //2.445是js内部一个bug
            if (f == 2.445) {
                return 2.45;
            }
            var f = Math.round(x * 100) / 100;
            var s = f.toString();
            var rs = s.indexOf('.');
            if (rs < 0) {
                rs = s.length;
                s += '.';
            }
            while (s.length <= rs + 2) {
                s += '0';
            }
            return s;
        }

        ///根据权限控制财务对账是否显示
        function GetPermission() {
            var flagPermission1 = 0, flagPermission2 = 0; flagPermission3 = 0;
            var savePer = sessionStorage.getItem("perList");

            if (savePer == null || savePer == "") {
                //重新获取一次权限
                $.ajaxSetup({
                    async: false
                });
                $.ajax({
                    url: '@Url.Content("~/Account/GetPermissionJson/")',
                    data: { EmployeeNo: employeeInfo.EmployeeNo },
                    datatype: "json",
                    async: false,
                    type: "GET",
                    success: function (result) {
                        try {
                            if (result) {
                                sessionStorage.setItem("perList", result);

                            }

                        } catch (ex) {
                            submitMessageShow(ex.message.toString());
                        }
                    }
                });
                $.ajaxSetup({
                    async: true
                });
                savePer = sessionStorage.getItem("perList");


            }

            if (savePer) {
                var list = JSON.parse(sessionStorage.getItem("perList"));
                for (var i = 0; i < list.length; i++) {
                    if (list[i].ACTION == "FEE" && list[i].NAME != "记账员") {
                        //这是一个审批人
                        flagPermission1 = 1;
                        if (list[i].NAME == "财务会计") {
                            //这是财务会计
                            flagPermission2 = 1;
                        }
                        else if (list[i].NAME == "中心负责人" || list[i].NAME == "片区负责人") {
                            flagPermission3 = 1;
                        }
                    }
                    if (list[i].ACTION == "FEE" && list[i].NAME == "后台管理") {
                        $("#li10").show();
                    }
                }
            }
            if (flagPermission1) {
                $("#li100").show();
                $("#li101").show();
            }
            if (flagPermission2) {
                $("#li102").show();
                $("#li11").show();
            }
            if (flagPermission3) {
                $("#li102").show();
            }
        }


